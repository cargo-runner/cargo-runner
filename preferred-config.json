{
  "version": "2.0",
  "build_system": "Cargo",
  "frameworks": {
    "test": "cargo-test",
    "binary": {
      "command": "cargo",
      "channel": "nightly",
      "subcommand": "leptos watch",
      "composition": "{env} {command} {subcommand} {args} {extra_args} -- {extra_test_binary_args}",
      "args": [],
      "env": {},
      "extra_args": [],
      "extra_test_binary_args": []
    }
  },
  "overrides": [
    {
      "match": {
        "package": "my-web-app",
        "file_path": "src/main.rs",
        "symbol_name": "main",
        "module_path": "tests::test_user"
      },
      "frameworks": {
        "test": "cargo-nextest"
      },
      "extra_args": ["--release"],
      "extra_test_binary_args": ["--test-threads=1"],
      "extra_env": {
        "DATABASE_URL": "postgres://localhost/test_db"
      }
    }
  ]
}
