{
  "cargo": {
    "package": "project-a",
    "channel": "nightly",
    "extra_args": ["--features", "test-feature"],
    "extra_env": {
      "RUST_LOG": "debug"
    }
  },
  "rustc": {
    "extra_args": ["--edition", "2021"],
    "extra_env": {
      "RUST_BACKTRACE": "1"
    },
    "test_framework": {
      "build": {
        "command": "rustc",
        "args": ["--test", "{source_file}", "-o", "{output_name}"],
        "extra_args": ["--cfg", "test"]
      },
      "exec": {
        "command": "./{output_name}",
        "args": ["{test_name}"],
        "extra_args": [],
        "extra_test_binary_args": ["--nocapture", "--test-threads=1"]
      }
    },
    "binary_framework": {
      "build": {
        "command": "rustc",
        "args": ["--crate-type", "bin", "--crate-name", "{crate_name}", "{source_file}", "-o", "{output_name}"],
        "extra_args": []
      },
      "exec": {
        "command": "./{output_name}",
        "args": [],
        "extra_args": []
      }
    }
  },
  "single_file_script": {
    "extra_args": []
  },
  "overrides": [
    {
      "match": {
        "function_name": "test_user"
      },
      "cargo": {
        "extra_test_binary_args": ["--test-threads=1"],
        "extra_env": {
          "SPECIAL_TEST": "true"
        }
      }
    },
    {
      "match": {
        "module_path": "tests"
      },
      "cargo": {
        "extra_args": ["--nocapture"],
        "test_framework": {
          "command": "cargo",
          "subcommand": "nextest run",
          "extra_args": ["-j4"]
        }
      }
    },
    {
      "match": {
        "module_path": "User"
      },
      "cargo": {
        "extra_args": ["--doc", "--features", "doc-test"],
        "channel": "stable"
      }
    }
  ]
}