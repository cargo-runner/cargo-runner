{
  "cargo": {
    "package": "project-a",
    "channel": "nightly",
    "extra_args": ["--features", "test-feature"],
    "extra_env": {
      "RUST_LOG": "debug"
    }
  },
  "overrides": [
    {
      "match": {
        "function_name": "test_user"
      },
      "cargo": {
        "extra_test_binary_args": ["--test-threads=1"],
        "extra_env": {
          "SPECIAL_TEST": "true"
        }
      }
    },
    {
      "match": {
        "module_path": "tests"
      },
      "cargo": {
        "extra_args": ["--nocapture"],
        "test_framework": {
          "command": "cargo",
          "subcommand": "nextest run",
          "extra_args": ["-j4"]
        }
      }
    },
    {
      "match": {
        "module_path": "User"
      },
      "cargo": {
        "extra_args": ["--doc", "--features", "doc-test"],
        "channel": "stable"
      }
    }
  ]
}