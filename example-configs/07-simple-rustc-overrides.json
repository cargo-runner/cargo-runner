{
  "rustc": {
    "extra_args": ["--edition", "2021"],
    "extra_test_args": ["--cfg", "test"],
    "extra_test_binary_args": ["--no-capture"],
    "extra_env": {
      "RUST_BACKTRACE": "1"
    }
  },
  "overrides": [
    {
      "match": {
        "file_type": "Standalone"
      },
      "rustc": {
        "extra_args": ["-O"]
      }
    },
    {
      "match": {
        "file_type": "Standalone",
        "file_path": "/Users/uriah/Code/nodes/test.rs"
      },
      "rustc": {
        "extra_args": ["--verbose"],
        "extra_test_args": ["--cfg", "debug_mode"],
        "extra_env": {
          "TEST_MODE": "debug"
        }
      }
    },
    {
      "match": {
        "file_type": "Standalone",
        "function_name": "test_alpha"
      },
      "rustc": {
        "extra_test_binary_args": ["--exact", "--show-output"]
      }
    },
    {
      "match": {
        "file_type": "Standalone",
        "module_path": "tests"
      },
      "rustc": {
        "extra_test_binary_args": ["--test-threads=1"]
      }
    }
  ]
}