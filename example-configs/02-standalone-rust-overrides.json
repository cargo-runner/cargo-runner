{
  "rustc": {
    "extra_args": ["--edition", "2021", "-O"],
    "extra_env": {
      "RUST_BACKTRACE": "1"
    },
    "test_framework": {
      "build": {
        "command": "rustc",
        "args": ["--test", "{source_file}", "-o", "{output_name}"],
        "extra_args": ["--cfg", "test", "--edition", "2021"]
      },
      "exec": {
        "command": "./{output_name}",
        "args": ["{test_name}"],
        "extra_test_binary_args": ["--nocapture", "--test-threads=1"]
      }
    },
    "binary_framework": {
      "build": {
        "command": "rustc",
        "args": ["--crate-type", "bin", "--crate-name", "{crate_name}", "{source_file}", "-o", "{output_name}"],
        "extra_args": ["-O", "--edition", "2021"]
      },
      "exec": {
        "command": "./{output_name}",
        "args": []
      }
    }
  },
  "overrides": [
    {
      "match": {
        "file_type": "Standalone",
        "file_path": "/Users/uriah/Code/nodes/test.rs"
      },
      "rustc": {
        "extra_args": ["--cfg", "feature=\"debug\""],
        "extra_env": {
          "RUST_LOG": "debug"
        }
      }
    },
    {
      "match": {
        "file_type": "Standalone",
        "file_path": "/Users/uriah/Code/nodes/test.rs",
        "module_path": "tests"
      },
      "rustc": {
        "test_framework": {
          "exec": {
            "extra_test_binary_args": ["--show-output", "--nocapture"]
          }
        }
      }
    },
    {
      "match": {
        "file_type": "Standalone",
        "file_path": "/Users/uriah/Code/nodes/test.rs",
        "function_name": "test_alpha"
      },
      "rustc": {
        "extra_args": ["--cfg", "alpha_test"],
        "test_framework": {
          "build": {
            "extra_args": ["--verbose"]
          },
          "exec": {
            "extra_test_binary_args": ["--exact", "--nocapture"]
          }
        }
      }
    },
    {
      "match": {
        "file_type": "Standalone",
        "file_path": "/Users/uriah/Code/nodes/perf.rs"
      },
      "rustc": {
        "extra_args": ["-C", "opt-level=3", "-C", "target-cpu=native"],
        "binary_framework": {
          "build": {
            "extra_args": ["-C", "lto=fat"]
          }
        }
      }
    }
  ]
}