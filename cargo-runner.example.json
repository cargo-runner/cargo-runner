{
  "overrides": [
    {
      "matcher": {
        "package": "my_crate",
        "module_path": "*::tests::*"
      },
      "cargo_args": ["--release"],
      "exec_args": ["--nocapture"],
      "env": {
        "RUST_LOG": "debug"
      }
    },
    {
      "matcher": {
        "function_name": "bench_*"
      },
      "cargo_args": ["--release"]
    },
    {
      "matcher": {
        "file_path": "**/integration_tests/**"
      },
      "cargo_args": ["--features", "integration"],
      "env": {
        "TEST_ENV": "integration"
      }
    }
  ],
  "cache_enabled": true,
  "cache_dir": "/tmp/cargo-runner-cache"
}