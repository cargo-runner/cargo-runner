{
  "cargo": {
    "package": "windrunner",
    "features": ["core", "logging"],
    "extra_env": {
      "RUST_LOG": "debug"
    },
    "test_framework": {
      "command": "cargo",
      "subcommand": "test",
      "features": ["test-utils"]
    }
  },
  "overrides": [
    {
      "match": {
        "file_path": "examples/showcase.rs"
      },
      "cargo": {
        "features": "all"
      }
    },
    {
      "match": {
        "file_path": "src/lib.rs"
      },
      "cargo": {
        "features": ["minimal"]
      },
      "force_replace_features": true
    }
  ]
}